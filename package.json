{
  "name": "Prunidor-Back-NestJS",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=development nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "start:prod:pm2": "npm run build && env-cmd -f ./.env.production pm2 start ecosystem.config.js",
    "docker:dev": "env-cmd -f ./.env.development docker-compose -f docker-compose.dev.yml up -d",
    "docker:prod": "env-cmd -f ./.env.production docker-compose -f docker-compose.prod.yml up -d",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "rimraf dist && cross-env NODE_ENV=test vitest run",
    "test:watch": "rimraf dist && cross-env NODE_ENV=test vitest --watch",
    "test:cov": "rimraf dist && cross-env NODE_ENV=test vitest --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "rimraf dist && cross-env NODE_ENV=test vitest --run --config ./vitest.config.e2e.ts",
    "test:e2e:github": "rimraf dist && cross-env NODE_ENV=github vitest --run --config ./vitest.config.e2e.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:run": "cross-var env-cmd -f ./.env.$npm_config_env ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d ./src/config/typeorm-cli.ts migration:run",
    "migration:generate": "cross-var env-cmd -f ./.env.$npm_config_env ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d ./src/config/typeorm-cli.ts migration:generate ./src/migrations/$npm_config_name",
    "migration:create": "cross-var env-cmd -f ./.env.$npm_config_env ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./src/migrations/$npm_config_name",
    "migration:revert": "cross-var env-cmd -f ./.env.$npm_config_env ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d ./src/config/typeorm-cli.ts migration:revert"
  },
  "_exampleCommand": {
    "vitest start test": "yarn run test:e2e -- plots.e2e-spec.ts",
    "typeorm:migration": "npm run migration:create --env=development --name=your_migration_name",
    "create:deep-ressource": "schematics deep-resource-nestjs-schematics:deep-resource --name=visual-control --path=api/",
    "create:test": "schematics deep-resource-nestjs-schematics:prunidor-tests --name=visual-control --base-path=/crop_notebook/"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.9.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.1.14",
    "@nestjs/terminus": "^10.1.1",
    "@nestjs/typeorm": "^10.0.0",
    "axios": "^1.5.1",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.2.4",
    "cache-manager-redis-yet": "^4.1.2",
    "class-sanitizer": "^1.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "nestjs-form-data": "^1.9.1",
    "node-cron": "^3.0.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "pm2": "^5.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc/core": "^1.3.101",
    "@types/cache-manager": "^4.0.4",
    "@types/compression": "^1",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.9",
    "@types/node": "^20.3.1",
    "@types/node-cron": "^3",
    "@types/passport-jwt": "^3.0.11",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.1.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "leaked-handles": "^5.2.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "unplugin-swc": "^1.4.4",
    "vite-tsconfig-paths": "^4.2.2",
    "vitest": "^1.1.0"
  },
  "packageManager": "yarn@4.0.2"
}
